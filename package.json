{
  "name": "school-schedule",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec 'clear && npm run babel-node src/index.js'",
    "babel-node": "babel-node --presets='@babel/preset-env' --ignore='foo|bar|baz'",
    "clean": "rm -rf build && mkdir build",
    "build-server": "npx babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-server && npm run sync && npm run seed",
    "start": "node ./build/index.js",
    "seed": "node ./build/database/seed.js",
    "sync": "node ./build/database/sync.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "apollo-server-express": "^2.22.2",
    "bcrypt": "^5.0.1",
    "core-js": "^3.11.0",
    "cors": "^2.8.5",
    "express": "~4.16.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.12",
    "chalk": "^3.0.0",
    "eslint": "^7.24.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "sequelize-auto": "^0.8.2"
  }
}
